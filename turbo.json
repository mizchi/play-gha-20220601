{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "globalDependencies": [
    "pnpm-lock.yaml"
  ],
  "pipeline": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "typecheck": {
      "dependsOn": ["build"]
    },
    "test": {
      "dependsOn": ["build"]
    }
  }
}
